{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon -x ts-node index.ts",
    "debug": "nodemon -x ts-node index.ts --debug",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && npm run copy-json",
    "db:migrate": "ts-node ./node_modules/.bin/sequelize-cli db:migrate",
    "db:migrate:undo": "ts-node ./node_modules/.bin/sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "ts-node ./node_modules/.bin/sequelize-cli db:migrate:undo:all",
    "db:seed:init": "ts-node ./node_modules/.bin/sequelize-cli db:seed --seed init-data.ts",
    "db:seed:refresh:perm": "ts-node ./node_modules/.bin/sequelize-cli db:seed:undo --seed permission.ts && ts-node ./node_modules/.bin/sequelize-cli db:seed --seed permission.ts",
    "db:seed:all": "ts-node ./node_modules/.bin/sequelize-cli db:seed:all",
    "db:create": "ts-node ./node_modules/.bin/sequelize-cli db:create",
    "format": "prettier --write .",
    "copy-json": "copyfiles -u 1 src/**/*.json build/src/"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csv-parse": "^5.5.6",
    "dayjs": "^1.11.12",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.1",
    "express-list-routes": "^1.2.2",
    "express-validator": "^7.1.0",
    "fs": "^0.0.1-security",
    "generate-password": "^1.7.1",
    "html-pdf": "^3.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mammoth": "^1.8.0",
    "md5": "^2.3.0",
    "mongodb": "^6.8.0",
    "mongoose": "^8.5.0",
    "mysql2": "^3.11.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.14",
    "pdf-parse": "^1.1.1",
    "qrcode": "^1.5.4",
    "sequelize": "^6.37.3",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@meanie/mongoose-to-json": "^2.6.0",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.0",
    "@types/lodash": "^4.17.6",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.17.6",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^10.0.0",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.4",
    "path": "^0.12.7",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  }
}
